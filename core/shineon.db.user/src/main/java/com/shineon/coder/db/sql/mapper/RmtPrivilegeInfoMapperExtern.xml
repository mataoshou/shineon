<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.shineon.coder.db.sql.mergedao.IRmtPrivilegeInfoMapper" >
    <select id="list" resultMap="BaseResultMap" >
        select
        <include refid="Base_Column_List" />
        from rmt_privilege_info
        <if test="where != null">
         where ${where}
        </if>
        <if test="order != null">
            order by ${order}
        </if>
    </select>
    <select id="selectByName" resultMap="BaseResultMap" parameterType="java.lang.String" >
        select
        <include refid="Base_Column_List" />
        from rmt_privilege_info 
        where privilegeName = #{name,jdbcType=VARCHAR}
    </select> 
    <insert id="insertByCustomId"   parameterType="com.shineon.coder.db.sql.pojo.RmtPrivilegeInfo" >
        insert into rmt_privilege_info (id,privilegeName,description,showOrder,reserved1,reserved2)
        values (#{id,jdbcType=VARCHAR},#{privilegename,jdbcType=VARCHAR},#{description,jdbcType=VARCHAR},#{showorder,jdbcType=INTEGER},#{reserved1,jdbcType=INTEGER},#{reserved2,jdbcType=VARCHAR})
    </insert>
</mapper>